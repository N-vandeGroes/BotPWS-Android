<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/BOTSELECTOR.LABELFRAME"
        android:padding="12.5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/BOTSELECTOR.LABELFRAME.LABEL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/back_color"
            android:text="@string/BOTSELECTOR.LABELFRAME.LABEL" />
        
    </LinearLayout>

    <LinearLayout
        android:id="@+id/BOTSELECTOR.LISTHOLDER"
        android:padding="12.5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    
	    <ListView
	        android:id="@+id/BOTSELECTOR.SELECTORLIST"
	        android:layout_width="match_parent"
	    	android:layout_height="wrap_content" >
	    </ListView>

	</LinearLayout>
    
</LinearLayout>
